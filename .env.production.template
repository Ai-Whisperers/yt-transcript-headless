# Production Environment Configuration
# Copy this file to .env.production and configure for your environment

# Application
NODE_ENV=production
PORT=3000

# Security
ENABLE_STEALTH=true
CORS_ORIGIN=*

# Rate Limiting
RATE_LIMIT_WINDOW=60000
RATE_LIMIT_MAX=10

# Logging
LOG_LEVEL=info

# Browser Configuration
MAX_CONCURRENT_BROWSERS=5
TIMEOUT_MS=30000
