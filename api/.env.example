# Server Configuration
PORT=3000
NODE_ENV=development

# CORS Configuration
CORS_ORIGIN=*

# Rate Limiting
RATE_LIMIT_WINDOW=60000
RATE_LIMIT_MAX=10

# Logging
LOG_LEVEL=info

# Browser Configuration
MAX_CONCURRENT_BROWSERS=5
TIMEOUT_MS=30000
ENABLE_STEALTH=true

# Extraction Settings
MAX_RETRIES=3
EXTRACTION_TIMEOUT=30000